    {% block stylesheets %}
    {% endblock %}
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    {% include 'BraincraftedBootstrapBundle::ie8-support.html.twig' %}
</head>
<body>
    {% block body %}{% endblock %}
